<layer title="已忽略补丁" width="700" height="520">
  <header class="mainhead" width="full" height="50">
    <icon src="100" pos="5,5"/>
    <imgbtn id="10000" class="linkimage" skin="closebtn" pos="-50,0"/>
    <text class="dlgtitle" pos="25,5">已忽略补丁</text>
  </header>
  
  <footer class="mainfoot" width="full" height="50">
    <text class="linktext" pos="10,10" href="http://www.beike.cn/?s=1">什么是已忽略补丁</text>
  </footer>
  
  <body class="mainbody" width="full" height="full">
    <dlg id="200" pos="0,0,-0,-0">
      
      <!-- 正在扫描 -->
      <dlg id="201" pos="0,0,-0,-0">
        <img id="105" pos="0,0" skin="examani" sub="0"/>
        <text pos="70,0" class="examtitle">正在扫描电脑安全状况，请稍候 ...</text>
        <progress id="112" pos="70,25,-65,45" min="0" max="200" value="0" showpercent="1"/>
        <button id="1001"  pos="-60,25,-0,45" class="uglybtn">取消</button>
      </dlg>
      
      <!-- 列表 -->
      <dlg id="300" show="0" pos="0,0,-0,-0">
        <text id="30000"  pos="10,10">找到 %d 个被忽略的补丁。您可以取消忽略，之后它们将可以被重新安装</text>
        <realwnd id="30010" ctrlid="30010" pos="5,35,-5,-40"/>
        
        <dlg id="305"  pos="5,-35,-5,-5">
          <text id="30500" pos="10,10" class="linktext">全选</text>
          <text id="30501" pos="40,10" class="linktext">全不选</text>
          <button id="30504" pos="-225,10" class="uglybtn">重新扫描</button>
          <button id="30502" pos="-165,10" class="uglybtn">取消忽略</button>
          <button id="30503" pos="-100,10" class="uglybtn">关闭</button>
        </dlg>
      </dlg>
      
    </dlg>
    
  </body>
</layer>
